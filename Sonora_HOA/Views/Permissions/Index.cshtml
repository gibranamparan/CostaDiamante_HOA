@model IEnumerable<Sonora_HOA.Models.Permissions>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.condo.name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.startDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.guestID)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.condo.name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.startDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.guestID)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.permissionsID }) |
            @Html.ActionLink("Details", "Details", new { id=item.permissionsID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.permissionsID })
        </td>
    </tr>
}

</table>
